#!/bin/sh
#SBATCH -o scheduler_stdout.txt    # Name of stdout output file(%j expands to jobId)
#SBATCH -e scheduler_stderr.txt    # Name of stderr output file(%j expands to jobId)
#SBATCH --partition=compute           # submit to the 'large' queue for jobs > 256 nodes
#SBATCH --qos=nsg
#SBATCH -L nsg:1
#SBATCH -J NGBW-JOB-NEURON74_TG-155F52BADDD64B3D93323AC7FE8C1C16        # Job name
#SBATCH -t 00:30:00         # Run time (hh:mm:ss) - 1.5 hours
#SBATCH --mail-user=nsgprod@sdsc.edu
#SBATCH --mail-type=begin
#SBATCH --mail-type=end
#The next line is required if the user has more than one project
# #SBATCH -A A-yourproject  # Allocation name to charge job against
#SBATCH -A csd403
# #SBATCH -A csd403  # Allocation name to charge job against ..add account after lib.email
cd '/projects/ps-nsg/home/nsguser/ngbw/workspace/NGBW-JOB-NEURON74_TG-155F52BADDD64B3D93323AC7FE8C1C16/SIMPLETEST'

echo Job starting at `date` > ../start.txt
curl https://nsgdev.sdsc.edu:8443/portal2/taskupdate.action?taskId=25675\&status=START
export CIPRES_THREADSPP=4
export CIPRES_NP=4
#export NPROCS=`wc -l < $PBS_NODEFILE`
/projects/ps-nsg/home/nsguser/ngbw/workspace/NGBW-JOB-NEURON74_TG-155F52BADDD64B3D93323AC7FE8C1C16/./batch_command.cmdline 1>/projects/ps-nsg/home/nsguser/ngbw/workspace/NGBW-JOB-NEURON74_TG-155F52BADDD64B3D93323AC7FE8C1C16/stdout.txt 2>/projects/ps-nsg/home/nsguser/ngbw/workspace/NGBW-JOB-NEURON74_TG-155F52BADDD64B3D93323AC7FE8C1C16/stderr.txt
cd '/projects/ps-nsg/home/nsguser/ngbw/workspace/NGBW-JOB-NEURON74_TG-155F52BADDD64B3D93323AC7FE8C1C16'
/bin/tar -cvzf output.tar.gz ./*
#/bin/tar -cvzf ../output.tar.gz ../*
echo Job finished at `date` > done.txt
curl https://nsgdev.sdsc.edu:8443/portal2/taskupdate.action?taskId=25675\&status=DONE
